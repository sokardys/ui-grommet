{
  "name": "@sokardys/ui-grommet",
  "version": "0.4.0",
  "description": "Small library of grommet-based components",
  "main": "dist/ui-grommet.js",
  "module": "dist/ui-grommet.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "react",
    "grommet",
    "styled-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://git@github.com/sokardys/ui-grommet.git"
  },
  "bugs": "https://github.com/sokardys/ui-grommet/issues",
  "license": "UNLICENSED",
  "contributors": [
    {"name": "Sokardys"},
    {"name": "Whiteyhat"}
  ],
  "scripts": {
    "build": "microbundle-crl --css-modules false --no-compress --format modern,cjs",
    "prepare": "run-s clean build",
    "clean": "rimraf dist",
    "dev": "start-storybook -p 9001",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix | snazzy"
  },
  "dependencies": {
    "clipboard": "^2.0.6",
    "rc-tooltip": "^4.2.1",
    "react-intersection-observer": "^8.27.1",
    "use-debounce": "^3.4.3"
  },
  "peerDependencies": {
    "@google/model-viewer": "^1.1.0",
    "@hookform/resolvers": "^0.1.0",
    "flickity": "^2.2.1",
    "flickity-sync": "^2.0.0",
    "grommet": "^2.15.0",
    "grommet-icons": "^4.4.0",
    "jarallax": "^1.12.2",
    "next": "^9.5.2",
    "plyr": "^3.6.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-hook-form": "^6.5.1",
    "react-share": "^4.2.1",
    "reaptcha": "^1.7.2",
    "styled-components": "^5.1.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@google/model-viewer": "^1.1.0",
    "@hookform/resolvers": "^0.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "flickity": "^2.2.1",
    "flickity-sync": "^2.0.0",
    "grommet": "^2.15.0",
    "grommet-icons": "^4.4.0",
    "husky": "^4.2.5",
    "jarallax": "^1.12.2",
    "microbundle-crl": "^0.13.11",
    "next": "^9.5.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "plyr": "^3.6.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-hook-form": "^6.5.1",
    "react-is": "^16.13.1",
    "react-share": "^4.2.1",
    "reaptcha": "^1.7.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "styled-components": "^5.1.1",
    "yup": "^0.29.3"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run \"lint\""
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      ".cache",
      "dist"
    ],
    "globals": [
      "test",
      "expect",
      "import"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.20"
    },
    "componentsDefaultDirectory": "src/components/{name}",
    "packageManager": "yarn"
  }
